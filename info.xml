<?xml version="1.0"?>
<extension key="de.systopia.xcm" type="module">
  <file>xcm</file>
  <name>Extended Contact Matcher (XCM)</name>
  <description>CiviCRM contact matching extension</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.xcm</url>
    <url desc="Documentation">https://docs.civicrm.org/xcm/en/latest</url>
    <url desc="Support">https://github.com/systopia/de.systopia.xcm/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-07-11</releaseDate>
  <version>1.13.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Xcm</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Xcm_Upgrader</upgrader>
</extension>
